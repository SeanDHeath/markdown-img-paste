{
  "name": "markdown-image-paste",
  "main": "./lib/markdown-img-paste",
  "version": "2.0.1",
  "description": "Just use 'ctrl+v' paste image into markdown",
  "keywords": [
    "markdown",
    "md",
    "image",
    "paste"
  ],
  "activationCommands": {
    "atom-workspace": "markdown-img-paste:paste"
  },
  "repository": "https://github.com/nmecad/markdown-img-paste.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "configSchema": {
    "only_markdown": {
      "type": "boolean",
      "default": true,
      "title": "Only enable the Key-Combo in Markdown files",
      "description": ""
    },
    "use_subfolder": {
      "type": "boolean",
      "default": false,
      "title": "Save images in assets folder",
      "description": ""
    },
    "subfolder": {
      "type": "string",
      "default": "",
      "title": "Subfolder  for the images",
      "description": ""
    }
  }
}
