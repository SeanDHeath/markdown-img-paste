{
  "name": "markdown-image-paste",
  "main": "./lib/markdown-img-paste",
  "version": "2.0.0",
  "description": "Just use 'ctrl+shfit+v' paste image into markdown",
  "keywords": [
    "markdown",
    "md",
    "image",
    "paste"
  ],
  "activationCommands": {
    "atom-workspace": "markdown-img-paste:paste"
  },
  "repository": "https://github.com/cocoakekeyu/markdown-img-paste",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "qiniu": "^6.1.11",
    "request": "^2.55.0"
  },
  "configSchema": {
    "only_markdown": {
      "type": "boolean",
      "default": true,
      "title": "Only enable the Key-Combo in Markdown files",
      "description": ""
    },
    "use_assets_folder": {
      "type": "boolean",
      "default": true,
      "title": "Save images in assets folder",
      "description": ""
    }
  }
}
